# QNX MIPS big-endian toolchain

CC = qcc -V gcc_ntomipsbe
CXX = QCC -V gcc_ntomipsbe
AR = qcc -V gcc_ntomipsbe -A
ARFLAGS = 
LDFLAGS = -lsocket

RANLIB = ntomips-ranlib
STRIP = ntomips-strip

# qcc uses -M for map files
MKDEP_CPP = ntomips-gcc -E

LORDER = echo
TSORT = cat


