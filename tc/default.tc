#
# Default toolchain.  Edit to match your platform.
#
CC = cc
CXX = g++
CFLAGS = -O -pipe
CXXFLAGS = ${CFLAGS}
AR = ar
ARFLAGS = rl
RANLIB = ranlib
LD = ld 
LDFLAGS = __UNSET__
ECHO = echo
MKDEP = mkdep
STRIP = strip
STRIP_FLAGS = -x
INSTALL = install
INSTALL_COPY = -c
INSTALL_STRIP = -s
MKINSTALLDIRS = ${MAKL_DIR}/helpers/mkinstalldirs
LORDER = lorder
TSORT = tsort
# avoid circular dependencies error
PRE_LDADD = __UNSET__
POST_LDADD = __UNSET__
