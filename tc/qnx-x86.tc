# QNX x86 toolchain

CC = qcc -V gcc_ntox86
CXX = QCC -V gcc_ntox86
AR = qcc -V gcc_ntox86 -A
ARFLAGS = 
LDFLAGS = -lsocket

RANLIB = ntox86-ranlib
STRIP = ntox86-strip

# qcc uses -M for map files
MKDEP_CPP = ntox86-gcc -E

LORDER = echo
TSORT = cat


