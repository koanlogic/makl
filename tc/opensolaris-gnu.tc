#
# GNU/OpenSolaris (gcc-dev suite installed)
#

CC = /usr/sfw/bin/gcc
CXX = /usr/sfw/bin/g++
AR = /usr/sfw/bin/gar
AS = /usr/sfw/bin/gas
LD = /usr/sfw/bin/gld
RANLIB = /usr/sfw/bin/granlib

# no --{start,end}-group (NOTE: leave a blank after '=')
PRE_LDADD = __UNSET__
POST_LDADD = __UNSET__

ECHO = /usr/gnu/bin/echo

# need SUNWscp package
INSTALL = /usr/ucb/install

LORDER = /usr/bin/lorder
TSORT = /usr/gnu/bin/tsort

MKINSTALLDIRS = /bin/mkdir -p
