# QNX MIPS little-endian toolchain

CC = qcc -V gcc_ntomipsle
CXX = QCC -V gcc_ntomipsle
AR = qcc -V gcc_ntomipsle -A
ARFLAGS = 
LDFLAGS = -lsocket

RANLIB = ntomips-ranlib
STRIP = ntomips-strip

# qcc uses -M for map files
MKDEP_CPP = ntomips-gcc -E

