#
# tested on FreeBSD 5.*
#
CC = cc
CFLAGS = -pipe
AR = ar
ARFLAGS = rl
RANLIB = ranlib
LD = ld 
LDFLAGS = 
MKDEP = mkdep
STRIP = strip
STRIP_FLAGS = -x
INSTALL = install
INSTALL_COPY = -c
INSTALL_STRIP = -s
MKINSTALLDIRS = /bin/mkdir -p
LORDER = lorder
# avoid circular dependencies error
PRE_LDADD = -Wl,--start-group
POST_LDADD = -Wl,--end-group
