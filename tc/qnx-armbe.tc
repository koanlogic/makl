# QNX ARM big-endian toolchain

CC = qcc -V gcc_ntoarmbe
CXX = QCC -V gcc_ntoarmbe
AR = qcc -V gcc_ntoarmbe -A
ARFLAGS = 
LDFLAGS = -lsocket

RANLIB = ntoarm-ranlib
STRIP = ntoarm-strip

# qcc uses -M for map files
MKDEP_CPP = ntoarm-gcc -E

